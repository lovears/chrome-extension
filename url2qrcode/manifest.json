{
  "manifest_version": 2,

  "name": "url2qrcode",
  "description": "url转二维码",
  "version": "1.0",

  // "default_locale": "zh_CN",
  
    "background": {
      "scripts": ["js/jquery.min.js","js/qrcode.min.js","js/background.js","qrcode.js"],
      "persistent": false
    },

    "browser_action": {
      "default_popup": "qrcode.html"
    },
  "homepage_url": "https://lovears.github.io",
  "permissions": [
      "tabs"
  ]
  
}
